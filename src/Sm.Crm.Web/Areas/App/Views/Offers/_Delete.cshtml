@model OfferDto

<form asp-action="DeleteCustomerItem" asp-route-id="@Model.OfferStatusId" role="form">
  @await Html.PartialAsync("_ModalHeader", new ModalHeader { Heading = "Delete Offer" })
  <div class="modal-body form-horizontal">
    Are you want to delete this task?
  </div>
  @await Html.PartialAsync("_ModalFooter", new ModalFooter { SubmitButtonText = "Delete" })
</form>
